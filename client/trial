            function leftScroll(){
              const left = document.getElementById("gallery")
              left.scrollLeft = left.scrollLeft - scrollAmt
            }
            function rightScroll(){
              const right = document.getElementById("gallery")
              right.scrollLeft = right.scrollLeft + scrollAmt
            }